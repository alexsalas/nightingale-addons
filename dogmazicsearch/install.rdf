<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">

    <em:id>dogmazicsearch@geekshadow.com</em:id>
    <em:name>Dogmazic Search</em:name>
    <em:description>Adds Dogmazic to the list of search engines</em:description>
    <em:creator>Antoine Turmel (geekshadow@gmail.com)</em:creator>
    <em:homepageURL>http://geekshadow.com/</em:homepageURL>
    <em:version>1.0.9</em:version>
    <em:iconURL>chrome://dogmazicsearch/skin/icon.png</em:iconURL>
    <em:type>2</em:type>

    <!-- Songbird --> 
    <em:targetApplication>
      <Description>
        <em:id>songbird@songbirdnest.com</em:id>
        <em:minVersion>1.4.3</em:minVersion>
        <em:maxVersion>2.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    
    <!-- Nightingale --> 
    <em:targetApplication>
      <Description>
        <em:id>nightingale@getnightingale.com</em:id>
        <em:minVersion>1.11.0a</em:minVersion>
        <em:maxVersion>2.3.0a</em:maxVersion>
      </Description>
    </em:targetApplication>    

  </Description>

</RDF>
