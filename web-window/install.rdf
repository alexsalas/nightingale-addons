<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#"
     xmlns:songbird="http://www.songbirdnest.com/2007/addon-metadata-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>web-window@geekshadow.com</em:id>
    <em:type>2</em:type>
    <em:name>Web Window</em:name>
    <em:version>0.5</em:version>
    <em:creator>Antoine Turmel (geekshadow@gmail.com)</em:creator>
    <em:contributor>Original code : mybrowser</em:contributor>
    <em:description>Allow browsing in a new window</em:description>
    <em:iconURL>chrome://web-window/skin/icon.png</em:iconURL>

    <!-- Songbird -->
    <em:targetApplication>
      <Description>
        <em:id>songbird@songbirdnest.com</em:id>
        <em:minVersion>1.7.0a</em:minVersion>
        <em:maxVersion>2.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    
    <!-- Nightingale -->
    <em:targetApplication>
      <Description>
        <em:id>nightingale@getnightingale.com</em:id>
        <em:minVersion>1.11.0a</em:minVersion>
        <em:maxVersion>2.3.0a</em:maxVersion>
      </Description>
    </em:targetApplication>    

  </Description>
</RDF>
